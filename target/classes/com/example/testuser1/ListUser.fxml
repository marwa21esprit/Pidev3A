<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #F7F4EA;" stylesheets="@design.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.testuser1.ListUserController">
   <children>
      <TableView fx:id="tableuser" prefHeight="313.0" prefWidth="600.0" style="-fx-background-color: #F7F4EA;" styleClass="gradient" stylesheets="@design.css">
        <columns>
          <TableColumn fx:id="username" prefWidth="47.33334481716156" text="UserName" />
          <TableColumn fx:id="email" prefWidth="61.33333969116211" text="Email" />
            <TableColumn fx:id="address" prefWidth="72.00003814697266" text="address" />
            <TableColumn fx:id="role" prefWidth="62.0" text="Role" />
            <TableColumn fx:id="question" prefWidth="55.33335876464844" text="Question" />
            <TableColumn fx:id="Answer" prefWidth="94.0" text="Answer" />
            <TableColumn fx:id="Status" prefWidth="440.0" text="Status" />
        </columns>
      </TableView>
      <Button fx:id="deleteuser" layoutX="29.0" layoutY="341.0" mnemonicParsing="false" onAction="#deleteuser" styleClass="login-btn" text="Delete" />
      <Button fx:id="finduser" layoutX="374.0" layoutY="337.0" mnemonicParsing="false" onAction="#finduser" styleClass="login-btn" stylesheets="@design.css" text="Find" textFill="WHITE" />
      <TextField fx:id="UserNameTF" layoutX="439.0" layoutY="342.0" promptText="Enter UserName" styleClass="textfield" />
      <Button fx:id="Tri_btn" layoutX="329.0" layoutY="339.0" mnemonicParsing="false" onAction="#Tri" styleClass="login-btn" text="Tri" />
      <Button layoutX="104.0" layoutY="340.0" mnemonicParsing="false" onAction="#Desactivatebtn" styleClass="login-btn" text="Desactivate" />
   </children>
</AnchorPane>
